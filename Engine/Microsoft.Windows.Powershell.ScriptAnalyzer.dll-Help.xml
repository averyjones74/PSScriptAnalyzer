<?xml version="1.0" encoding="utf-8"?>

<helpItems xmlns="http://msh" schema="maml">

  <command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10">
    <command:details>
      <command:name>Invoke-ScriptAnalyzer</command:name>
      <command:verb>Invoke</command:verb>
      <command:noun>ScriptAnalyzer</command:noun>
    </command:details>
    <maml:description>
      <maml:para>Initialize ScriptAnalyzer and analyze the specified file given the specified rules. </maml:para>
  	</maml:description>
    <command:examples>
      <command:example>
        <maml:title>----------  EXAMPLE 1  ----------</maml:title>
        <dev:code> Invoke-ScriptAnalyzer -ScriptFilePath c:\test.ps1</dev:code>
        <dev:remarks>
          <maml:para> If no rules are specified, ScriptAnalyer is going to perform an analysis on the given .ps file using default built in 				rules</maml:para>
        </dev:remarks>
      </command:example>
    </command:examples>
  </command:command>

</helpItems>